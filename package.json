{
	"name": "@mockden/source",
	"version": "0.0.0",
	"private": true,
	"license": "MIT",
	"scripts": {
		"biome": "npx @biomejs/biome check .",
		"prepare": "husky",
		"db:generate:dev": "drizzle-kit generate --config=drizzle-dev.config.ts",
		"db:migrate:dev": "drizzle-kit migrate --config=drizzle-dev.config.ts",
		"db:seed": "npx tsx --env-file=.env.development apps/backend/src/db/seed.ts",
		"backend:dev": "nx run @mockden/backend:serve & ngrok http 3000",
		"robots:prod": "cp apps/frontend/public/robots.production.txt apps/frontend/public/robots.txt",
		"robots:dev": "cp apps/frontend/public/robots.development.txt apps/frontend/public/robots.txt",
		"prebuild": "npm run robots:prod",
		"predev": "npm run robots:dev"
	},
	"dependencies": {
		"@clerk/backend": "^2.1.0",
		"@clerk/clerk-react": "^5.32.0",
		"@clerk/express": "^1.7.0",
		"@monaco-editor/react": "^4.7.0",
		"@neondatabase/serverless": "^1.0.0",
		"@tailwindcss/postcss": "^4.1.8",
		"@tailwindcss/vite": "^4.1.8",
		"@tanstack/react-form": "^1.11.3",
		"@tanstack/react-query": "^5.80.7",
		"@tanstack/react-router": "^1.121.12",
		"@tanstack/react-router-devtools": "^1.121.12",
		"@types/express": "^4.17.21",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"drizzle-orm": "^0.44.0",
		"lucide-react": "^0.513.0",
		"randexp": "^0.5.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-router-dom": "6.29.0",
		"sqlite3": "^5.1.7",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.8",
		"vite-tsconfig-paths": "^5.1.4",
		"zod": "^3.25.28",
		"zustand": "^5.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@faker-js/faker": "^9.8.0",
		"@nx/esbuild": "^21.1.2",
		"@nx/jest": "21.1.2",
		"@nx/js": "21.1.2",
		"@nx/node": "^21.1.2",
		"@nx/react": "21.1.2",
		"@nx/vite": "21.1.2",
		"@nx/web": "21.1.2",
		"@nx/workspace": "21.1.2",
		"@swc-node/register": "~1.9.1",
		"@swc/cli": "~0.6.0",
		"@swc/core": "~1.5.7",
		"@swc/helpers": "~0.5.11",
		"@tanstack/router-plugin": "^1.121.12",
		"@testing-library/dom": "10.4.0",
		"@testing-library/react": "16.1.0",
		"@types/body-parser": "^1.19.5",
		"@types/cors": "^2.8.18",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.0.0",
		"@types/react": "19.0.0",
		"@types/react-dom": "19.0.0",
		"@vitejs/plugin-react": "^4.2.0",
		"@vitest/ui": "^3.0.0",
		"autoprefixer": "10.4.13",
		"babel-jest": "^29.7.0",
		"drizzle-kit": "^0.31.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jiti": "2.4.2",
		"jsdom": "~22.1.0",
		"lint-staged": "^16.1.0",
		"nx": "21.1.2",
		"postcss": "^8.4.38",
		"prettier": "^2.6.2",
		"ts-jest": "^29.1.0",
		"ts-node": "10.9.1",
		"tsconfig-paths": "^4.2.0",
		"tslib": "^2.3.0",
		"tsx": "^4.19.4",
		"tw-animate-css": "^1.3.4",
		"typescript": "~5.7.2",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"workspaces": ["apps/*"],
	"lint-staged": {
		"*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
			"biome check --files-ignore-unknown=true",
			"biome check --write --no-errors-on-unmatched",
			"biome check --write --organize-imports-enabled=false --no-errors-on-unmatched",
			"biome check --write --unsafe --no-errors-on-unmatched",
			"biome format --write --no-errors-on-unmatched",
			"biome lint --write --no-errors-on-unmatched"
		],
		"*": ["biome check --no-errors-on-unmatched --files-ignore-unknown=true"]
	}
}
